-static bool ends_with(struct String s, const char *suffix)
-{
-    int n = strlen(suffix);
-    if (n > s.string_n)
-    {
-        return false;
-    }
-    else
-    {
-        for (int i = 0; i < n; i++)
-        {
-            if (s.string[s.string_n - i - 1] != suffix[n - i - 1])
-                return false;
-        }
-        return true;
-    }
-}
