-    assert(p && i);
-    struct Parser p_ = *p;
-    unsigned char c;
-
-    *i = 0;
-    for (;;)
-    {
-        if (!peek_char(&p_, &c))
-            break;
-        if (!('0' <= c && c <= '9'))
