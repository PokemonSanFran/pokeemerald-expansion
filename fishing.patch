-__attribute__((warn_unused_result))
-static bool match_eol(struct Parser *p)
-{
-    struct Parser p_ = *p;
-    unsigned char c;
-
-    skip_whitespace(&p_);
-    for (;;)
-    {
-        if (!pop_char(&p_, &c))
-            return false;
-
-        if (c == '\n')
-            break;
-        else
-            return false;
-    }
-
-    *p = p_;
-    return true;
-}
