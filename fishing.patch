-static bool parse_section(struct Parser *p, struct Token *section)
-{
-    assert(p && section);
-    struct Parser p_ = *p;
-
-    if (!match_exact(&p_, "==="))
-        return false;
-
-    skip_whitespace(&p_);
-    if (!match_identifier(&p_, section))
