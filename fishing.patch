-__attribute__((warn_unused_result))
-static bool match_exact(struct Parser *p, const char *s)
-{
-    assert(p && s);
-    struct Parser p_ = *p;
-    unsigned char c;
-
-    for (; *s != '\0'; s++)
-    {
-        if (!pop_char(&p_, &c))
-            return false;
-        if (*s != c)
-            return false;
-    }
-
-    *p = p_;
-    return true;
-}
