-            if (!match_exact(&p_, ")"))
-                return set_parse_error(p, p_.location, "expected ')'");
-
-            *nickname = first;
-            *species = second;
-            *gender = third;
-        }
-        else
-        {
-            if (second.begin + 1 == second.end) // Single letter.
-            {
-                *nickname = (struct Token) {};
-                *species = first;
-                *gender = second;
-            }
-            else
-            {
-                *nickname = first;
-                *species = second;
-                *gender = (struct Token) {};
-            }
-        }
-    }
-    else
-    {
-        *nickname = (struct Token) {};
