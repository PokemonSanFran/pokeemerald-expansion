-static bool is_literal_string(struct String s1, const char *s2)
-{
-    for (int i = 0;; i++)
-    {
-        if (i == s1.string_n && s2[i] == '\0')
-            return true;
-        else if (i == s1.string_n || s2[i] == '\0')
-            return false;
-        else if (s1.string[i] != s2[i])
-            return false;
-    }
