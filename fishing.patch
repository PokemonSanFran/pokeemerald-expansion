-static void match_until_eol(struct Parser *p, struct Token *t)
-{
-    assert(p && t);
-    unsigned char c;
-
-    skip_whitespace(p);
-
-    t->source = p->source;
-    t->location = p->location;
-    t->begin = p->offset;
-    t->end = p->offset;
-
-    for (;;)
-    {
-        if (!peek_char(p, &c))
-            break;
-        if (c == '\n')
-            break;
-        if (!pop_char(p, &c))
-            assert(false);
-        if (c != ' ' && c != '\t')
-            t->end = p->offset;
-    }
-}
